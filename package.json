{
  "name": "ReactNativeShowcase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "run-devices": "yarn ios && yarn android",
    "pods": "pod-install --quiet",
    "test": "jest",
    "build-e2e-ios": "detox build --configuration ios",
    "build-e2e-android": "detox build --configuration android",
    "test-e2e-ios": "detox test --configuration ios --take-screenshots failing",
    "test-e2e-android": "detox test --configuration android --take-screenshots failing",
    "check-types": "tsc --noEmit",
    "lint": "eslint --max-warnings 0 --cache \"**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:check": "eslint --max-warnings 0 --cache \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@ftdr/react-native-auth": "file:.yalc/@ftdr/react-native-auth",
    "@ftdr/react-native-dynamic-links": "file:.yalc/@ftdr/react-native-dynamic-links",
    "@ftdr/react-native-notifications": "file:.yalc/@ftdr/react-native-notifications",
    "@mobily/stacks": "^2.0.0",
    "@react-native-async-storage/async-storage": "^1.16.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^14.4.0",
    "@react-native-firebase/dynamic-links": "^14.4.0",
    "@react-native-firebase/messaging": "^14.4.0",
    "@react-navigation/material-bottom-tabs": "^6.1.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@react-navigation/stack": "^6.1.1",
    "@storybook/react-native": "^5.3.25",
    "@types/react-native-video": "^5.0.12",
    "@xstate/react": "^2.0.0",
    "expo": ">=44.0.0-0 <45.0.0",
    "expo-local-authentication": "^12.1.1",
    "expo-random": "^12.1.2",
    "expo-secure-store": "^11.1.1",
    "expo-web-browser": "^10.1.1",
    "react": "17.0.2",
    "react-hook-form": "^7.28.1",
    "react-native": "0.66.4",
    "react-native-camera": "^4.2.1",
    "react-native-cardscan": "^2.0.0",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-image-picker": "^4.7.3",
    "react-native-paper": "^4.11.2",
    "react-native-reanimated": "^2.4.1",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.12.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^12.3.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-video": "^5.2.0",
    "react-query": "^3.34.14",
    "react-safe-context-hooks": "^1.0.1",
    "search-params": "^4.0.1",
    "xstate": "^4.30.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/runtime": "^7.17.2",
    "@ftdr/commitlint-config": "^0.1.0",
    "@ftdr/prettier-config": "^0.0.1",
    "@ftdr/sematic-release-config": "^0.1.0",
    "@ftdr/tsconfig": "^0.0.1",
    "@types/jest": "^27.4.0",
    "@types/react-native": "^0.66.15",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^19.4.4",
    "eslint": "^8.8.0",
    "eslint-config-universe": "^10.0.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "lint-staged": "^12.3.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "pod-install": "^0.1.31",
    "prettier": "^2.5.1",
    "react-native-dotenv": "^3.3.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "17.0.2",
    "styled-components": "^5.3.3",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "@types/react": "^17",
    "styled-components": "^5"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "e2e"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --cache --fix"
    ],
    "**/*.{js,jsx,json,ts,tsx}": [
      "prettier --write"
    ]
  }
}
